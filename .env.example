# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Orchestrator Routing Settings
ROUTING_LLM=openai
ROUTING_LLM_MODEL=gpt-4o-mini
ROUTING_LLM_TEMPERATURE=0.0

# Database Configuration
POSTGRES_DATABASE_URL=postgresql://username:password@localhost:5432/ai_agent_db
SQLITE_DATABASE_PATH=./data/agent_memory.db

# Memory Configuration
FAISS_INDEX_PATH=./data/faiss_index
BUFFER_MAX_SIZE=1000
VECTOR_DIMENSION=1536

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/agent.log

# Web Search Tool
SERPER_API_KEY=your_serper_api_key_here
BROWSERLESS_API_KEY=your_browserless_api_key_here

# Application Settings
DEFAULT_AGENT_ID=default_agent
DEFAULT_LLM_PROVIDER=openai
DEFAULT_LLM_MODEL=gpt-4o

